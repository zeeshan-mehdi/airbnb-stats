{
  "name": "BNB Sniper Pro",
  "description": "BNB Sniper Pro extension is for Airbnb Stats",
  "manifest_version": 2,
  "version": "0.1",
  "background": {
    "scripts": [
        "jquery.js",
        "background.js"
    ]
},
"browser_action": {
    "default_title": "BMI",
    "default_popup": "mobile.html",
    "default_icon": {
      "19": "bmi_19.png",
      "38": "bmi_32.png"
   }
},
"permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
],
  "icons": { "128": "bmi.png","64":"bmi_64.png" }
}